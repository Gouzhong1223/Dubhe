ENV = 'test'

# 默认BASE URL
VUE_APP_BASE_API = ''

# 用户 minio 访问地址
VUE_APP_MINIO_API = ''

# atlas 服务，需要单独部署
VUE_APP_ATLAS_HOST = ''

# 医疗影像 DCM4CHEE 服务访问地址
